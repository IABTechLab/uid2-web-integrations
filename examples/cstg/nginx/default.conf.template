server {
    listen       80;
    server_name  localhost;
    root   /usr/share/nginx/html;

    #access_log  /var/log/nginx/host.access.log  main;

    location / {
        ssi on;
        ssi_silent_errors off;
        set $SSI_UID_JS_SDK_URL "${UID_JS_SDK_URL}";
        set $SSI_UID_JS_SDK_NAME "${UID_JS_SDK_NAME}";
        set $SSI_UID_BASE_URL '${UID_BASE_URL}';
        set $SSI_SERVER_PUBLIC_KEY '${SERVER_PUBLIC_KEY}';
        set $SSI_SUBSCRIPTION_ID '${SUBSCRIPTION_ID}';
    }
}